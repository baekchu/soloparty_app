{
  "expo": {
    "name": "Solo Party",
    "slug": "solo-party-dating",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#fce7f3"
    },
    "description": "솔로들을 위한 파티 매칭 서비스",
    "primaryColor": "#ec4899",
    "privacy": "public",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.soloparty.dating",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "프로필 사진 업로드를 위해 필요합니다",
        "NSUserNotificationsUsageDescription": "새로운 파티 등록 알림과 광고 시청 가능 알림을 받으실 수 있습니다"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#fce7f3"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.soloparty.dating",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "e208b050-52b3-4957-94c7-227046e0409e"
      }
    }
  }
}
